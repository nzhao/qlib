<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<config>
    <Name>
        <SolutionName type="char">BenzeneDynamics</SolutionName>
    </Name>
    <Condition>
        <MagneticField type="eval">1e-4*[0 0 100]</MagneticField>
    </Condition>
    <Dynamics>
        <TimeList type="eval">0:20e-6:5e-3</TimeList>
    </Dynamics>
    <InitialState>
        <DensityMatrix type="char">1.0 * p(2)_1 * p(1)_2 * p(1)_3 * p(1)_4 * p(1)_5 * p(1)_6</DensityMatrix>
    </InitialState>
    <Interaction>
        <IsSecular type="double">1</IsSecular>
    </Interaction>
    <Observable>
        <ObservableNumber type="double">6</ObservableNumber>
        <ObservableName1 type="char">sz1</ObservableName1>
        <ObservableString1 type="char">1.0 * sz_1</ObservableString1>
        <ObservableName2 type="char">sz2</ObservableName2>
        <ObservableString2 type="char">1.0 * sz_2</ObservableString2>
        <ObservableName3 type="char">sz3</ObservableName3>
        <ObservableString3 type="char">1.0 * sz_3</ObservableString3>
        <ObservableName4 type="char">sz4</ObservableName4>
        <ObservableString4 type="char">1.0 * sz_4</ObservableString4>
        <ObservableName5 type="char">sz5</ObservableName5>
        <ObservableString5 type="char">1.0 * sz_5</ObservableString5>
        <ObservableName6 type="char">sz6</ObservableName6>
        <ObservableString6 type="char">1.0 * sz_6</ObservableString6>
    </Observable>
    <SpinCollection>
        <FileName type="char">+xyz/BenzeneProtonZPlane.xyz</FileName>
        <Source type="char">File</Source>
    </SpinCollection>
</config>