<?xml version="1.0" encoding="utf-8"?>
<config>
   <Name>
       <SolutionName type="char">ButaneDynamics</SolutionName>
   </Name>
   <Condition>
      <MagneticField type="eval">1e-4*[0 0 100]</MagneticField>
   </Condition>
   <Dynamics>
      <TimeList type="eval">0:5e-6:1e-3</TimeList>
   </Dynamics>
   <InitialState>
      <DensityMatrix type="char">1.0 * p(1)_1</DensityMatrix>
   </InitialState>
   <Interaction>
      <IsSecular type="double">1</IsSecular>
   </Interaction>
   <Observable>
      <ObservableName1 type="char">sz1</ObservableName1>
      <ObservableNumber type="double">1</ObservableNumber>
      <ObservableString1 type="char">1.0 * sz_1</ObservableString1>
   </Observable>
   <SpinCollection>
      <FileName type="char">+xyz/ButaneProton.xyz</FileName>
      <Source type="char">File</Source>
   </SpinCollection>
</config>